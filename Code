#LEVEL 1
    @bot.callback_query_handler(func=lambda callback: callback.data == 'level1')
    def level1(callback):
        bot.send_message(callback.message.chat.id, '–î—Ä—É–≥, –ø–æ–∑–¥—Ä–∞–≤–ª—è—é, –º—ã –ø—Ä–∏–±—ã–ª–∏ –∫ —Å–ª–µ–¥—É—é—â–∏–º —Ç–æ—á–∫–∞–º <b>"–ì–∞–ª–∞–∫—Ç–∏–∫–∞ –≤–∏–¥–æ–≤ –∏ –ø—Ä–æ–µ–∫—Ü–∏–π"</b> –∏ <b>"–ú–∞—Å—Ç–µ—Ä—Å–∫–∞—è —Ç—Ä–µ—Ö–º–µ—Ä–Ω–æ–≥–æ  –º–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏—è"</b>. –ó–¥–µ—Å—å —Ç—ã –Ω–∞—É—á–∏—à—Å—è —á–∏—Ç–∞—å –∏–∑–æ–º–µ—Ç—Ä—á—Å–∫–∏–µ –ø—Ä–æ–µ–∫—Ü–∏–∏ –∏ –ø–æ–ª—É—á–∏—à—å –Ω–∞–≤—ã–∫–∏ –≤ –∏—Ö –ø–æ—Å—Ç—Ä–æ–µ–Ω–∏–∏.', parse_mode='html')
        markup = types.InlineKeyboardMarkup()
        btn1 = types.InlineKeyboardButton('–¢–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª\n–∏ —Ä–µ—à–µ–Ω–∏–µ', url='https://youtu.be/PccYPgAtrNY?si=GylG4aEXNDfWRsB4')
        btn2 = types.InlineKeyboardButton('–ü—Ä–∞–∫—Ç–∏–∫–∞', callback_data='–ü—Ä–∞–∫—Ç–∏–∫–∞')
        markup.row(btn1, btn2)
        bot.send_message(callback.message.chat.id, '–≠—Ç–∞–ø ‚Ññ1\n–í —Ç–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–æ–º —ç—Ç–∞–ø–µ —Ç—ã —Ç–∞–∫–∂–µ —Å–º–æ–∂–µ—à—å –Ω–∞–π—Ç–∏ —Ä–µ—à–µ–Ω–∏–µ –∑–∞–¥–∞–Ω–∏–π', reply_markup=markup)

        @bot.callback_query_handler(func=lambda callback: callback.data =='–ü—Ä–∞–∫—Ç–∏–∫–∞')
        def exer(callback):
            markup = types.InlineKeyboardMarkup()
            btn = types.InlineKeyboardButton('–Ø –∑–∞–∫–æ–Ω—á–∏–ª', callback_data='level2')
            markup.add(btn)
            bot.send_message(callback.message.chat.id, '–£ —Ç–µ–±—è –µ—Å—Ç—å 90 –º–∏–Ω—É—Ç –Ω–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –∑–∞–¥–∞–Ω–∏–π. –ï—Å–ª–∏ —Ç—ã –≤—ã–ø–æ–ª–Ω–∏—à—å —Ä–∞–Ω—å—à–µ –Ω–∞–∑–Ω–∞—á–µ–Ω–Ω–æ–≥–æ —Å—Ä–æ–∫–∞, —Ç–æ –∂–º–∏ –∫–Ω–æ–ø–∫—É "–Ø –≥–æ—Ç–æ–≤". –í—Ä–µ–º—è –ø–æ—à–ª–æ, —É–¥–∞—á–∏!', reply_markup=markup)
            time.sleep(0.5)
            exe1 = open('–≠—Ç–∞–ø ‚Ññ 1.docx', 'rb')
            bot.send_document(callback.message.chat.id, exe1)

        # LEVEL 2
