# 5 question
    @bot.callback_query_handler(func=lambda callback: callback.data == 'continue4')
    def test5(callback):
        ruler = open('ruler.jpeg', 'rb')
        markup = types.InlineKeyboardMarkup()
        btn1 = types.InlineKeyboardButton('–õ–∏–Ω–µ–π–∫–∞', callback_data='ruler')
        btn2 = types.InlineKeyboardButton('–¶–∏—Ä–∫—É–ª—å', callback_data='circ')
        markup.row(btn1, btn2)
        btn3 = types.InlineKeyboardButton('–¢—Ä–∞–Ω—Å–ø–æ—Ä—Ç–∏—Ä', callback_data='trans')
        markup.row(btn3)
        bot.send_photo(callback.message.chat.id, ruler, '–ö–∞–∫ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è –≤—ã–¥–µ–ª–µ–Ω–Ω—ã–π –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç?', reply_markup=markup)

