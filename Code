        time.sleep(0.5)
        markup = types.InlineKeyboardMarkup()
        markup.add(types.InlineKeyboardButton('–î–∞!', callback_data='continue4'))
        bot.send_message(callback.message.chat.id, '–ü—Ä–æ–¥–æ–ª–∂–∞–µ–º?', reply_markup=markup)

