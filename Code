                                btn2 = types.InlineKeyboardButton('–ó–∞–¥–∞–Ω–∏—è', callback_data='comp_ex1')
                                markup.row(btn1, btn2)
                                btn3 = types.InlineKeyboardButton('–ü—Ä–∏–º–µ—Ä —Ä–µ—à–µ–Ω–∏—è', callback_data='examp1')
                                markup.row(btn3)
                                bot.send_message(callback.message.chat.id, '–ü–µ—Ä–≤—ã–π —Ä–∞–∑–¥–µ–ª –ö–æ–º–ø–∞—Å–∞.\n<em>–°–æ–≤–µ—Ç—É—é –ø—Ä–æ—Å–º–∞—Ç—Ä–∏–≤–∞—Ç—å –ø—Ä–∏–º–µ—Ä –≤—ã–≤–ø–æ–ª–Ω–µ–Ω–∏—è –∑–∞–¥–∞–Ω–∏–π –≤ —Å–∞–º–æ–º –Ω–∞—á–∞–ª–µ.</em>', reply_markup=markup, parse_mode='html')

                                @bot.callback_query_handler(func=lambda callback: callback.data == 'comp_base')
                                def comp_base(callback):
                                    base = open('–ë–∞–∑–∞ –∫–æ–º–ø–∞—Å–∞.docx', 'rb')
                                    bot.send_message(callback.message.chat.id, '–≠—Ç–æ –æ—Å–Ω–æ–≤–Ω–∞—è —Ç–µ–æ—Ä–∏—è –ø–æ –ö–æ–º–ø–∞—Å—É')
                                    bot.send_document(callback.message.chat.id, base)

                                @bot.callback_query_handler(func=lambda callback: callback.data == 'examp1')
                                def examp(callback):
                                    ex = open('—Ä–µ—à–µ–Ω–∏–µ 1 –∑–∞–¥–∞–Ω–∏—è.docx', 'rb')
                                    bot.send_message(callback.message.chat.id,'–ù–µ –±–æ–π—Å—è –ø—Ä–æ—Å–∏–¥–µ—Ç—å –Ω–∞ —ç—Ç–æ–º —ç—Ç–∞–ø–µ –±–æ–ª—å—à–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤—Ä–µ–º–µ–Ω–∏. –í–Ω–∏–∫–∞–π –≤ –∫–∞–∂–¥—É—é –¥–µ—Ç–∞–ª—å!')
                                    bot.send_document(callback.message.chat.id, ex)

                                @bot.callback_query_handler(func=lambda callback: callback.data == 'comp_ex1')
                                def ex1(callback):
                                    ex = open('1 –∑–∞–¥–∞–Ω–∏–µ.docx', 'rb')
                                    bot.send_message(callback.message.chat.id, '–≠—Ç–∏ –∑–∞–¥–∞–Ω–∏—è –Ω–µ –Ω–∞ –≤—Ä–µ–º—è, –≤—ã–ø–æ–ª–Ω—è–π –≤ —É–¥–æ–±–Ω–æ–º –¥–ª—è —Ç–µ–±—è —Ç–µ–º–ø–µ.')
                                    bot.send_document(callback.message.chat.id, ex)
                                    time.sleep(5)
                                    markup = types.InlineKeyboardMarkup()
                                    btn1 = types.InlineKeyboardButton('–ü–µ—Ä–µ–π—Ç–∏ –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –∑–∞–¥–∞–Ω–∏—é', callback_data='extra2')
                                    btn2 = types.InlineKeyboardButton('–ó–∞–∫–æ–Ω—á–∏—Ç—å –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏–µ', callback_data='end')
                                    markup.row(btn1, btn2)
                                    bot.send_message(callback.message.chat.id, '–ù–∞–¥–µ—é—Å—å, —Ç–µ–±–µ –±—ã–ª–æ –∏–Ω—Ç–µ—Ä–µ—Å–Ω–æ –≤—ã–ø–æ–ª–Ω—è—Ç—å —ç—Ç–∏ –∑–∞–¥–∞–Ω–∏—è! –ê —Å–µ–π—á–∞—Å —Ö–æ—á—É –ø—Ä–µ–¥–ª–æ–∂–∏—Ç—å —Ç–µ–±–µ –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å –µ—à—ë –æ–¥–Ω–∏. –ß—Ç–æ —Ç—ã –¥—É–º–∞–µ—à—å –ø–æ —ç—Ç–æ–º—É –ø–æ–≤–æ–¥—É?', reply_markup=markup)

                                    @bot.callback_query_handler(func=lambda callback: callback.data == 'extra2')
                                    def extra2(callback):
                                        markup = types.InlineKeyboardMarkup()
                                        btn1 = types.InlineKeyboardButton('–¢–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª –¥–ª—è 2 –∑–∞–¥–∞–Ω–∏—è', callback_data='base2')
                                        btn2 = types.InlineKeyboardButton('–ó–∞–¥–∞–Ω–∏—è', callback_data='ex2')
                                        markup.row(btn1, btn2)
                                        btn3 = types.InlineKeyboardButton('–ü—Ä–∏–º–µ—Ä —Ä–µ—à–µ–Ω–∏—è', callback_data='examp2')
                                        markup.row(btn3)
                                        bot.send_message(callback.message.chat.id, '–Ø —Ä–∞–¥, —á—Ç–æ —Ç–µ–±–µ –∏–Ω—Ç–µ—Ä–µ—Å–Ω–æ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ–µ –∏–∑—É—á–µ–Ω–∏–µ —Ç–µ–º—ã, –ø–æ—ç—Ç–æ–º—É –≤—ã—Å—ã–ª–∞—é —Ç–µ–±–µ –µ—â—ë –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤!', reply_markup=markup)

                                        @bot.callback_query_handler(func=lambda callback: callback.data == 'base2')
                                        def base2(callback):
                                            base = open('—Ç–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª.docx', 'rb')
                                            bot.send_message(callback.message.chat.id, '–í–Ω–∏–º–∞—Ç–µ–ª—å–Ω–æ –∏–∑—É—á–∏ —ç—Ç—É —Ç–µ–º—É!')
                                            bot.send_document(callback.message.chat.id, base)

                                        @bot.callback_query_handler(func=lambda callback: callback.data == 'examp2')
                                        def examp2(callback):
                                            ex = open('–ø—Ä–∏–º–µ—Ä —Ä–µ—à–µ–Ω–∏—è 2 –∑–∞–¥–∞–Ω–∏—è.docx', 'rb')
                                            bot.send_message(callback.message.chat.id, '–ü—Ä–æ—Å–º–æ—Ç—Ä–∏ –≤—Å–µ –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —ç—Ç–æ–≥–æ –∑–∞–¥–∞–Ω–∏—è!')
                                            bot.send_document(callback.message.chat.id, ex)

                                        @bot.callback_query_handler(func=lambda callback: callback.data == 'ex2')
                                        def ex2(callback):
                                            ex = open('2 –∑–∞–¥–∞–Ω–∏–µ.docx', 'rb')
                                            bot.send_message(callback.message.chat.id, '–ö–∞–∂–¥—ã–π —Ä–∞–∑ –∑–∞–¥–∞–Ω–∏—è –±—É–¥—É—Ç —Å—Ç–∞–Ω–æ–≤–∏—Ç—å—Å—è –≤—Å—ë —Å–ª–æ–∂–Ω–µ–µ –∏ —Å–ª–æ–∂–Ω–µ–µ, –Ω–æ –Ω–µ –±–æ–π—Å—è, –ø—Ä–∏ –≤–Ω–∏–º–∞—Ç–µ–ª—å–Ω–æ–º –ø—Ä–æ—á—Ç–µ–Ω–∏–∏ —Ç–µ–æ—Ä–∏–∏ –∏ –ø—Ä–æ—Å–º–æ—Ç—Ä–µ —Ä–µ—à–µ–Ω–∏—è –∑–∞–¥–∞–Ω–∏–π, –ø—Ä–æ–±–ª–µ–º –≤–æ–∑–Ω–∏–∫–Ω—É—Ç—å –Ω–µ –¥–æ–ª–∂–Ω–æ.')
                                            bot.send_document(callback.message.chat.id, ex)
                                            time.sleep(5)
                                            markup = types.InlineKeyboardMarkup()
                                            btn1 = types.InlineKeyboardButton('–ü–µ—Ä–µ–π—Ç–∏ –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –∑–∞–¥–∞–Ω–∏—é', callback_data='extra3')
                                            btn2 = types.InlineKeyboardButton('–ó–∞–∫–æ–Ω—á–∏—Ç—å –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏–µ', callback_data='end')
                                            markup.row(btn1, btn2)
                                            bot.send_message(callback.message.chat.id,'–£ –º–µ–Ω—è –µ—Å—Ç—å –µ—â—ë –∏–Ω—Ç–µ—Ä–µ—Å–Ω—ã–π –º–∞—Ç–µ—Ä–∏–∞–ª, –∫–æ—Ç–æ—Ä—ã–π –º–æ–∂–µ—Ç —Ç–µ–±–µ –ø–æ–Ω—Ä–∞–≤–∏—Ç—å—Å—è. –•–æ—á–µ—à—å –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å?', reply_markup=markup)

                                            @bot.callback_query_handler(func=lambda callback: callback.data == 'extra3')
                                            def extra3(callback):
                                                markup = types.InlineKeyboardMarkup()
                                                btn1 = types.InlineKeyboardButton('–¢–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∏–π –º–∞—Ç–µ—Ä–∏–∞–ª', callback_data='base3')
                                                btn2 = types.InlineKeyboardButton('–ó–∞–¥–∞–Ω–∏–µ', callback_data='exer3')
                                                markup.row(btn1, btn2)
                                                btn3 = types.InlineKeyboardButton('–ü—Ä–∏–º–µ—Ä —Ä–µ—à–µ–Ω–∏—è', callback_data='examp3')
                                                markup.row(btn3)
                                                bot.send_message(callback.message.chat.id,'–ö–∞–∫ —Ç–µ–±–µ –¥–∞—é—Ç—Å—è –∑–∞–¥–∞–Ω–∏—è? –ï—Å–ª–∏ —Ç—ã –ø—Ä–æ—à—ë–ª —Ç–∞–∫ –¥–∞–ª–µ–∫–æ, —ç—Ç–æ –∑–Ω–∞—á–∏—Ç, —á—Ç–æ —É —Ç–µ–±—è —Ç–æ—á–Ω–æ –µ—Å—Ç—å –≤—Å–µ —à–∞–Ω—Å—ã –Ω–∞ –ø–æ–±–µ–¥—É!', reply_markup=markup)

                                                @bot.callback_query_handler(func=lambda callback: callback.data == 'base3')
                                                def base3(callback):
                                                    base = open('—Ç–µ–æ—Ä–µ—Ç–∏—á–µ—Å–∫–∞—è —á–∞—Å—Ç—å 3 –∑–∞–¥–∞–Ω–∏—è.docx', 'rb')
                                                    bot.send_message(callback.message.chat.id,'–í—á–∏—Ç—ã–≤–∞–π—Å—è —Ö–æ—Ä–æ—à–µ–Ω—å–∫–æ!')
                                                    bot.send_document(callback.message.chat.id, base)

                                                @bot.callback_query_handler(func=lambda callback: callback.data == 'examp3')
                                                def examp3(callback):
                                                    ex = open('—Ä–µ—à–µ–Ω–∏–µ 3 –∑–∞–¥–∞–Ω–∏—è.docx', 'rb')
                                                    bot.send_message(callback.message.chat.id, '–¢—É—Ç –≤—Å—ë –ø–æ–¥—Ä–æ–±–Ω–æ –æ–ø–∏—Å–∞–Ω–æ, –Ω–∞–¥–æ –ø—Ä–æ—Å—Ç–æ —Å–∫–æ–Ω—Ü–µ–Ω—Ç—Ä–∏—Ä–æ–≤–∞—Ç—å—Å—è')
                                                    bot.send_document(callback.message.chat.id, ex)

                                                @bot.callback_query_handler(func=lambda callback: callback.data == 'exer3')
                                                def exer3(callback):
                                                    exer = open('3 –∑–∞–¥–∞–Ω–∏–µ.docx', 'rb')
                                                    bot.send_message(callback.message.chat.id, '–ù–∏ –ø—É—Ö–∞, –Ω–∏ –ø–µ—Ä–∞!')
                                                    bot.send_document(callback.message.chat.id, exer)
                                                    time.sleep(30)
                                                    markup = types.InlineKeyboardMarkup()
                                                    markup.add(types.InlineKeyboardButton('–î–∞', callback_data= 'yes'))
                                                    bot.send_message(callback.message.chat.id, '–¢—ã –∑–∞–∫–æ–Ω—á–∏–ª?', reply_markup=markup)

                                                    @bot.callback_query_handler(func=lambda callback: callback.data == 'yes')
                                                    def concl(callback):
                                                        bot.send_message(callback.message.chat.id, '–ö–∞–∫ –±—ã –º–Ω–µ –Ω–µ —Ö–æ—Ç–µ–ª–æ—Å—å —ç—Ç–æ–≥–æ –≥–æ–≤–æ—Ä–∏—Ç—å, –Ω–æ –≤–æ—Ç –∏ –∑–∞–∫–æ–Ω—á–∏–ª–æ—Å—å –≤—Å—ë —Ç–æ, —á—Ç–æ —è –ø–æ–¥–≥–æ—Ç–æ–≤–∏–ª –¥–ª—è —Ç–µ–±—è. –ù–µ –±—É–¥–µ–º –ø—Ä–æ—â–∞—Ç—å—Å—è –Ω–∞ –≥—Ä—É—Å—Ç–Ω–æ–π –Ω–æ—Ç–µ! –ñ–µ–ª–∞—é —Ç–µ–±–µ —É–¥–∞—á–∏ –Ω–∞ –ø—Ä–µ–¥—Å—Ç–æ—è—â–∏—Ö —Å–æ—Ä–µ–≤–Ω–æ–≤–∞–Ω–∏—è—Ö. –ë—É–¥—å —É–≤–µ—Ä–µ–Ω–Ω—ã–º –≤ —Å–µ–±–µ, —Ç—ã —Å–æ –≤—Å–µ–º —Å–ø—Ä–∞–≤–∏—à—å—Å—è!')

                            @bot.callback_query_handler(func=lambda callback: callback.data == 'end')
                            def end(callback):
                                bot.send_message(callback.message.chat.id, '–ö–∞–∫ –±—ã –º–Ω–µ –Ω–µ —Ö–æ—Ç–µ–ª–æ—Å—å —ç—Ç–æ–≥–æ –≥–æ–≤–æ—Ä–∏—Ç—å, –Ω–æ –≤–æ—Ç –∏ –∑–∞–∫–æ–Ω—á–∏–ª–æ—Å—å –≤—Å—ë —Ç–æ, —á—Ç–æ —è –ø–æ–¥–≥–æ—Ç–æ–≤–∏–ª –¥–ª—è —Ç–µ–±—è. –ù–µ –±—É–¥–µ–º –ø—Ä–æ—â–∞—Ç—å—Å—è –Ω–∞ –≥—Ä—É—Å—Ç–Ω–æ–π –Ω–æ—Ç–µ! –ñ–µ–ª–∞—é —Ç–µ–±–µ —É–¥–∞—á–∏ –Ω–∞ –ø—Ä–µ–¥—Å—Ç–æ—è—â–∏—Ö —Å–æ—Ä–µ–≤–Ω–æ–≤–∞–Ω–∏—è—Ö. –ë—É–¥—å —É–≤–µ—Ä–µ–Ω–Ω—ã–º –≤ —Å–µ–±–µ, —Ç—ã —Å–æ –≤—Å–µ–º —Å–ø—Ä–∞–≤–∏—à—å—Å—è!')



bot.polling(none_stop=True)
