import telebot
from telebot import types
import time
from config import bot_token
bot = telebot.TeleBot(bot_token)


@bot.message_handler(commands=['start'])
def start(message):
    bot.send_message(message.chat.id,'–ü—Ä–∏–≤–µ—Ç! –Ø - –±–æ—Ç, —Å–æ–∑–¥–∞–Ω–Ω—ã–π –¥–ª—è —Å–æ–≤–µ—Ä—à–µ–Ω—Å—Ç–≤–æ–≤–∞–Ω–∏—è –Ω–∞–≤—ã–∫–æ–≤ –º–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏—è —É—á–µ–Ω–∏–∫–æ–≤ —Ä–∞–∑–ª–∏—á–Ω—ã—Ö –∫–ª–∞—Å—Å–æ–≤ –∏ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–π. –Ø –º–æ–≥—É –ø–æ–º–æ—á—å —Ç–µ–±–µ —É–ª—É—á—à–∏—Ç—å —Å–æ–±—Å—Ç–≤–µ–Ω–Ω—ã–µ –Ω–∞–≤—ã–∫–∏, –Ω–∞—É—á–∏—Ç—å —Ç–µ–±—è –ø–æ–ª—å–∑–æ–≤–∞—Ç—å—Å—è —Ä–∞–∑–ª–∏—á–Ω—ã–º–∏ –ø—Ä–æ–≥—Ä–∞–º–º–∞–º–∏ –∏ —Ç–∞–∫–∂–µ –ø–æ–ª—É—á–∏—Ç—å —Ü–µ–Ω–Ω—ã–π –æ–ø—ã—Ç –≤ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–∏ –ø—Ä–∞–∫—Ç–∏—á–µ—Å–∫–∏—Ö –∑–∞–¥–∞–Ω–∏–π. –Ø —Å—Ç–∞–Ω—É –Ω–µ–∑–∞–º–µ–Ω–∏–º—ã–º –ø–æ–º–æ—â–Ω–∏–∫–æ–º –≤ –ø–æ–¥–≥–æ—Ç–æ–≤–∫–µ –∫ —Ä–∞–∑–ª–∏—á–Ω—ã–º –∫–æ–Ω–∫—É—Ä—Å–∞–º –∏ –º–∞—Ä–∞—Ñ–æ–Ω–∞–º. –£–≤–µ—Ä–µ–Ω, —á—Ç–æ —Å–º–æ–≥—É –ø–æ–º–æ—á—å —Ç–µ–±–µ –¥–æ–±–∏—Ç—å—Å—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤, –Ω–µ—Å–º–æ—Ç—Ä—è –Ω–∞ —Ç–æ, –∫–∞–∫–æ–π —É —Ç–µ–±—è –±–∞–∑–æ–≤—ã–π —É—Ä–æ–≤–µ–Ω—å –∑–Ω–∞–Ω–∏–π! –ì–æ—Ç–æ–≤ –ø—Ä–∏—Å—Ç—É–ø–∏—Ç—å –∫ —Ä–∞–±–æ—Ç–µ?')
    markup = types.InlineKeyboardMarkup()
    btn1 = types.InlineKeyboardButton(text='3D –ú–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ', callback_data='3D –ú–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ')
    markup.add(btn1)
    bot.send_message(message.chat.id, '–í—ã–±–µ—Ä–∏ –∫–æ–º–∞–Ω–¥—É', reply_markup=markup)

@bot.callback_query_handler(func = lambda callback: callback.data == '3D –ú–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ')
def photo(callback):
    with open('ph.jpg', 'rb') as f:
        bot.send_photo(callback.message.chat.id, f, '<b>–ß—Ç–æ —É–º–µ–µ—Ç —ç—Ç–æ—Ç –±–æ—Ç?</b>\n–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é —Ç–µ–±—è, –¥—Ä—É–≥ –Ω–∞ –±–æ—Ä—Ç—É 3DüíªExpress.\n–ú–µ–Ω—è –∑–æ–≤—É—Ç –ú–∏–∫–æ –∏ —è –±—É–¥—É —Ç–≤–æ–∏–º –ø—Ä–æ–≤–æ–¥–Ω–∏–∫–æ–º –≤ –∑–∞—Ö–≤–∞—Ç—ã–≤–∞—é—â–µ–º –ø—Ä–∏–∫–ª—é—á–µ–Ω–∏–∏ –ø–æ –º–∞—Ä—à—Ä—É—Ç—É "3D-–∫–æ–Ω—Ç–µ–Ω—Ç".\n–û—Ç–ø—Ä–∞–≤–ª—è–µ–º—Å—è –≤ –ø—É—Ç—å. –Ø –ø—Ä–∏–≥–æ—Ç–æ–≤–∏–ª –¥–ª—è —Ç–µ–±—è –∫–∞—Ä—Ç—É –º–∞—Ä—à—Ä—É—Ç–∞, –≤ –Ω–µ–π —Å–æ–±—Ä–∞–Ω—ã –≤—Å–µ –ø–æ–∑–Ω–∞–≤–∞—Ç–µ–ª—å–Ω—ã–µ —Å—Ç–∞–Ω—Ü–∏–∏, –∫–æ—Ç–æ—Ä—ã–µ –º—ã –ø–æ—Å–µ—Ç–∏–º, –ø–æ–ª—É—á–∏–º –∑–Ω–∞–Ω–∏—è –∏  –ø–æ–ª–µ–∑–Ω—ã–µ –Ω–∞–≤—ã–∫–∏.', parse_mode='html')
    time.sleep(1)
    with open('map.jpg', 'rb') as m:
        bot.send_photo(callback.message.chat.id, m, '<b>–í–æ—Ç –∫–∞—Ä—Ç–∞ –Ω–∞—à–µ–≥–æ —É–≤–ª–µ–∫–∞—Ç–µ–ª—å–Ω–æ–≥–æ –ø—É—Ç–µ—à–µ—Å—Ç–≤–∏—è:</b>'
                                                    '\n<b>1. –í–æ—Ä–æ—Ç–∞ –∑–Ω–∞–Ω–∏–π -</b>\n–ø–æ–ª—É—á–∞–µ–º –≤–≤–æ–¥–Ω—ã–µ –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏–∏'
                                                    '\n<b>2. –ü–ª–∞–Ω–µ—Ç–∞ —ç—Å–∫–∏–∑–æ–≤ –∏ —á–µ—Ä—Ç–µ–∂–µ–π -</b> –∑–Ω–∞–∫–æ–º–∏–º—Å—è —Å –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–º –∏ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞–º–∏ –≤–∏—Ä—Ç—É–∞–ª—å–Ω–æ–π –ª–∞–±–æ—Ä–∞—Ç–æ—Ä–∏–∏ –ú–≠–® "–¢–µ—Ö–Ω–æ–ª–æ–≥–∏—è. –ß–µ—Ä—á–µ–Ω–∏–µ" '
                                                    '\n<b>3. –ì–∞–ª–∞–∫—Ç–∏–∫–∞ –≤–∏–¥–æ–≤ –∏ –ø—Ä–æ–µ–∫—Ü–∏–π -</b> –Ω–∞—É—á–∏–º—Å—è —á–∏—Ç–∞—Ç—å –∏–∑–æ–º–µ—Ç—Ä–∏—á–µ—Å–∫–∏–µ –ø—Ä–æ–µ–∫—Ü–∏–∏, —Ä–∞–∑–ª–∏—á–∞—Ç—å –≤–∏–¥—ã –º–æ–¥–µ–ª–∏'
                                                    '\n<b>4. –ú–∞—Å—Ç–µ—Ä—Å–∫–∞—è —Ç—Ä–µ—Ö–º–µ—Ä–Ω–æ–≥–æ  –º–æ–¥–µ–ª–∏—Ä–æ–≤–∞–Ω–∏—è -</b> –ø–æ–ª—É—á–∏–º –∞–ª–≥–æ—Ä–∏—Ç–º –ø–æ—Å—Ç—Ä–æ–µ–Ω–∏—è –º–æ–¥–µ–ª–∏ –ø–æ –∏–∑–æ–º–µ—Ç—Ä–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–µ–∫—Ü–∏–∏ –∏ —Ä–∞–∑–º–µ—Ä–∞–º '
                                                    '\n<b>5. –ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ —Ä–µ—Å—É—Ä—Å–æ–≤ -</b> —Å–æ—Ö—Ä–∞–Ω–∏–º –∏—Å—Ç–æ—á–Ω–∏–∫–∏ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤'
                                                    '\n–ö–∞–∂–¥—ã–π —É—Ä–æ–∫ –æ—Ç–∫—Ä–æ–µ—Ç –¥–ª—è —Ç–µ–±—è –Ω–æ–≤—ã–µ –≥–æ—Ä–∏–∑–æ–Ω—Ç—ã –≤ —Å–æ–∑–¥–∞–Ω–∏–∏ —á–µ—Ä—Ç–µ–∂–µ–π –∏ 3d –º–æ–¥–µ–ª–µ–π! –í–ø–µ—Ä–µ–¥, –∫ –Ω–æ–≤—ã–º –æ—Ç–∫—Ä—ã—Ç–∏—è–º!', parse_mode='html')
    time.sleep(0.5)
    mark_int = types.InlineKeyboardMarkup()
    btn_int1 = types.InlineKeyboardButton('–û—Ç–∫—Ä—ã—Ç—å –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é', url='https://storage.yandexcloud.net/portal-public/videos/tech_drawing-tutorial.mp4')
    btn_int2 = types.InlineKeyboardButton('–¢—Ä–µ–Ω–∏—Ä–æ–≤–æ—á–Ω—ã–π —Ç–µ—Å—Ç', callback_data='test')
    mark_int.row(btn_int1, btn_int2)
    btn_int3 = types.InlineKeyboardButton('–ü—Ä–∏—Å—Ç—É–ø–∏—Ç—å –∫ –∑–∞–¥–∞–Ω–∏—è–º ‚Ññ1 —ç—Ç–∞–ø–∞', callback_data='level1')
    mark_int.add(btn_int3)
    bot.send_message(callback.message.chat.id, '–î–ª—è –Ω–∞—á–∞–ª–∞ –ø–æ—Å–º–æ—Ç—Ä–∏ –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—é —Å —Ü–µ–Ω–Ω–æ–π –≤–≤–æ–¥–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–µ–π, –∫–æ—Ç–æ—Ä–∞—è –ø–æ–Ω–∞–¥–æ–±–∏—Ç—Å—è —Ç–µ–±–µ, —á—Ç–æ–±—ã –≤—ã–ø–æ–ª–Ω—è—Ç—å –∑–∞–¥–∞–Ω–∏—è –Ω–∞ –∫–∞–∂–¥–æ–π —Å—Ç–∞–Ω—Ü–∏–∏.', reply_markup=mark_int)
    time.sleep(0.5)
    bot.send_message(callback.message.chat.id, '–°–∫–æ—Ä–æ —è –≤–µ—Ä–Ω—É—Å—å –∑–∞ —Ç–æ–±–æ–π - –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –∏–∑—É—á–∏ –∫–∞—Ä—Ç—É –º–∞—Ä—à—Ä—É—Ç–∞ –∏ –ø—Ä–æ–π–¥–∏ –≤–≤–æ–¥–Ω—ã–π –∏–Ω—Å—Ç—Ä—É–∫—Ç–∞–∂!')
    @bot.callback_query_handler(func=lambda callback: callback.data == 'test')
    def test1(callback):
        bot.send_message(callback.message.chat.id,'–î—Ä—É–≥, –ø–æ–∑–¥—Ä–∞–≤–ª—è—é!\n–¢—ã –ø—Ä–æ—à–µ–ª –≤–≤–æ–¥–Ω—ã–π —É—Ä–æ–∫, –∞ –º—ã —É–∂–µ –ø—Ä–∏–±—ã–≤–∞–µ–º –Ω–∞ —Å—Ç–∞–Ω—Ü–∏—é <b>"–ü–ª–∞–Ω–µ—Ç–∞ —ç—Å–∫–∏–∑–æ–≤ –∏ —á–µ—Ä—Ç–µ–∂–µ–π".</b>', parse_mode='html')
        time.sleep(0.5)
        list = open('list.jpeg', 'rb')
        markup = types.InlineKeyboardMarkup()
        btn1 = types.InlineKeyboardButton('–õ–∏—Å—Ç', callback_data='list')
        btn2 = types.InlineKeyboardButton('–ü–æ–ª–µ', callback_data='field')
        markup.add(btn1, btn2)
        bot.send_photo(callback.message.chat.id, list, '–ö–∞–∫ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è –≤—ã–¥–µ–ª–µ–Ω–Ω–∞—è –æ–±–ª–∞—Å—Ç—å?', reply_markup=markup)

    @bot.callback_query_handler(func=lambda callback: callback.data == 'list')
    def res1l(callback):
        bot.send_message(callback.message.chat.id, '–≤–µ—Ä–Ω–æ, —ç—Ç–æ –õ–ò–°–¢ (—Ä–∞–±–æ—á–∞—è –∑–æ–Ω–∞ —á–µ—Ä—á–µ–Ω–∏—è)')
        time.sleep(0.5)
        markup = types.InlineKeyboardMarkup()
        markup.add(types.InlineKeyboardButton('–î–∞!', callback_data='continue'))
        bot.send_message(callback.message.chat.id, '–ü—Ä–æ–¥–æ–ª–∂–∞–µ–º?', reply_markup=markup)

    @bot.callback_query_handler(func=lambda callback: callback.data == 'field')
    def res1f(callback):
        bot.send_message(callback.message.chat.id, '–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ! –≠—Ç–æ –õ–ò–°–¢ (—Ä–∞–±–æ—á–∞—è –∑–æ–Ω–∞ —á–µ—Ä—á–µ–Ω–∏—è)')
        time.sleep(0.5)
        markup = types.InlineKeyboardMarkup()
        markup.add(types.InlineKeyboardButton('–î–∞!', callback_data='continue'))
        bot.send_message(callback.message.chat.id, '–ü—Ä–æ–¥–æ–ª–∂–∞–µ–º?', reply_markup=markup)

    @bot.callback_query_handler(func=lambda callback: callback.data == 'continue')
    def test2(callback):
        line = open('test2.jpeg', 'rb')
        markup2 = types.InlineKeyboardMarkup()
        btn2_1 = types.InlineKeyboardButton('–¢–µ–∫—Å—Ç', callback_data='txt')
        btn2_2 = types.InlineKeyboardButton('–õ–∏–Ω–∏—è –æ–±—Ä—ã–≤–∞', callback_data='lin')
        markup2.row(btn2_1, btn2_2)
        btn2_3 = types.InlineKeyboardButton('–õ–∞—Å—Å–æ', callback_data='las')
        markup2.row(btn2_3)
        bot.send_photo(callback.message.chat.id, line, '–ö–∞–∫ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è –≤—ã–¥–µ–ª–µ–Ω–Ω—ã–π –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç?', reply_markup=markup2)

    @bot.callback_query_handler(func=lambda callback: callback.data == 'txt')
    def res2t(callback):
        bot.send_message(callback.message.chat.id, '–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ! –≠—Ç–æ –õ–ò–ù–ò–Ø –û–ë–†–´–í–ê')
        time.sleep(0.5)
        markup = types.InlineKeyboardMarkup()
        markup.add(types.InlineKeyboardButton('–î–∞!', callback_data='continue2'))
        bot.send_message(callback.message.chat.id, '–ü—Ä–æ–¥–æ–ª–∂–∞–µ–º?', reply_markup=markup)

