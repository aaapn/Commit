#4 question
    @bot.callback_query_handler(func=lambda callback: callback.data == 'continue3')
    def test4(callback):
        cl = open('clean.jpeg', 'rb')
        markup = types.InlineKeyboardMarkup()
        btn1 = types.InlineKeyboardButton('–û—á–∏—â–∞–µ—Ç –ª–∏—Å—Ç', callback_data='clean')
        btn2 = types.InlineKeyboardButton('–ó–∞–∫—Ä—ã–≤–∞–µ—Ç –ª–∏—Å—Ç', callback_data='close')
        markup.row(btn1, btn2)
        btn3 = types.InlineKeyboardButton('–ö—Ä–∞—Å–∏—Ç', callback_data='paint')
        markup.row(btn3)
        bot.send_photo(callback.message.chat.id, cl, '–ß—Ç–æ –¥–µ–ª–∞–µ—Ç –¥–∞–Ω–Ω–∞—è –∫–Ω–æ–ø–∫–∞?', reply_markup=markup)

# 4 answer
    @bot.callback_query_handler(func=lambda callback: callback.data == 'clean')
    def res4c(callback):
