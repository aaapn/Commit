# 3 question
    @bot.callback_query_handler(func=lambda callback: callback.data == 'continue2')
    def test3(callback):
        kyl = open('kyl.jpeg', 'rb')
        markup = types.InlineKeyboardMarkup()
        btn1 = types.InlineKeyboardButton('–ö—É–ª—å–º–∞–Ω', callback_data='kyl')
        btn2 = types.InlineKeyboardButton('–õ–∏–Ω–µ–π–∫–∞', callback_data='rul')
        markup.row(btn1, btn2)
        btn3 = types.InlineKeyboardButton('–õ–∏–Ω–µ–π–∫–∞ –¥–ª—è —É–≥–ª–æ–≤', callback_data='ang')
        markup.row(btn3)
        bot.send_photo(callback.message.chat.id, kyl, '–ö–∞–∫ –Ω–∞–∑—ã–≤–∞–µ—Ç—Å—è –≤—ã–¥–µ–ª–µ–Ω–Ω—ã–π –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç?', reply_markup=markup)

#3 answer
    @bot.callback_query_handler(func=lambda callback: callback.data == 'rul')
    def res3r(callback):
